{
  "name": "moviet/opcache",
  "description": "Simple laravel opcache using artisan commands for most versions",
  "license": "MIT",
  "keywords": [
    "opcache",
    "laravel",
    "php",
    "artisan"
  ],
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Moviet\\Opcache\\": "src/"
    }
  },
  "require": {
    "php": "^7.0 || ^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Moviet\\Opcache\\OpcodeServiceProvider"
      ]
    }
  }
}